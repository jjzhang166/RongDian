<?xml version="1.0" encoding="UTF-8"?>
<Window size="300,200" sizebox="2,2,2,2" caption="0,0,0,30" roundcorner="4,4,6,6" alpha="250" >
	<Font name="微软雅黑" size="10" bold="false" default="true"/>
	<Font name="微软雅黑" size="12" bold="false"/>
	<!-- frame_container -->
	<VerticalLayout name="frame_container" bordersize="1" borderround="2,2" inset="1,1,1,1" >
		<!-- frame_title_contaner -->
		<HorizontalLayout name="frame_title_contaner" mouse="ture" height="23">
	   		<Control width="3"/> <!-- Left Blank -->
			<Button name="frame_logo" width="20" height="20" padding="2" normalimage="file='default\RongDian.png'" tooltip="" />
	   		<Control width="5"/> <!-- Right Blank -->
			<!-- frame_title -->
	   		<Label name="frame_title" font="1" text="" minwidth="160" /> <!-- frame_title -->
			<!-- System's Button -->
	   		<HorizontalLayout height="23" >
				<Control /> <!-- Blank -->
				<Button name="closebtn" width="39" height="20" normalimage="file='default\btn_close_normal.png'" hotimage="file='default\btn_close_highlight.png'" pushedimage="file='default\btn_close_down.png'" />   
	   		</HorizontalLayout><!-- System's Button -->
	 	</HorizontalLayout><!-- frame_title_contaner -->
		<!-- frame_contents -->
		<HorizontalLayout name="frame_log" inset="2,2,2,2" >
			<Control width="5" />
			<Text name="update_log" text="" />
			<Control width="5" />
	 	</HorizontalLayout><!-- frame_contents -->
		<HorizontalLayout name="frame_progress" height="22" inset="2,2,2,2" visible="false">
			<Control width="10" />
			<Label name="update_progress_text" width="60" text="" />
			<Control width="3" />
			<Progress name="update_progress" bkimage="file='default\process_bg.png'" foreimage="file='default\process_fg.png'" max="100" min="0" value="0" text="0%" />
			<Control width="10" />
	 	</HorizontalLayout><!-- frame_contents -->
		<!-- frame_footer -->
		<HorizontalLayout name="frame_footer" height="23" >
			<Control />
			<Button name="update_ok" width="60" text="" normalimage="file='default\btn_normal.png'" hotimage="file='default\btn_hot.png'" pushedimage="file='default\btn_push.png'" bordersize="1" bordercolor="#FFE8E8E8" borderround="2,2,2,2" visible="false"/>
			<Control width="5"/>
			<Button name="update_cancel" width="60" text="" normalimage="file='default\btn_normal.png'" hotimage="file='default\btn_hot.png'" pushedimage="file='default\btn_push.png'" bordersize="1" bordercolor="#FFE8E8E8" borderround="2,2,2,2" />
			<Control width="5" />
		</HorizontalLayout> <!-- frame_footer -->
		<Control height="5" />
	</VerticalLayout> <!-- frame_container -->
</Window>