<?xml version="1.0" encoding="UTF-8"?>
<Window >
	<HorizontalLayout name="panel_ipconfig" inset="4,4,4,4" >
		<VerticalLayout name="ipconfig_setting" minwidth="320" bordersize="1" bordercolor="FFE0E0E0" >
			<HorizontalLayout height="24" bordersize="1" bordercolor="FFE0E0E0" >
				<Control width="5" />
				<Label name="ipconfig_setting_list_text" width="80" />
				<Control width="3" />
				<Combo name="ipconfig_solution" tooltip="" normalimage="file='default\combo_normal.png' corner='2,2,24,2'" hotimage="file='default\combo_hover.png' corner='2,2,24,2'" pushedimage="file='default\combo_hover.png' corner='2,2,24,2'" itemtextpadding="3,3,3,3" >
				</Combo>
			</HorizontalLayout>
			<Control height="10" />
			<HorizontalLayout height="24" bordersize="1" bordercolor="FFE0E0E0" >
				<Control width="15" />
				<Label name="ipconfig_s_name_text" width="100" />
				<Edit name="ipconfig_s_name_edit" bkcolor="#FFE0E0E0" nativebkcolor="#FFE0E0E0"/>
				<Control width="2" />
			</HorizontalLayout>
			<Control height="5" />
			<VerticalLayout height="136" bordersize="1" bordercolor="FFE0E0E0" >
				<CheckBox name="ipconfig_ip_auto" height="25" textpadding="25,5,0,0" textcolor="#FF324F6D" hottextcolor="#FF0000FF" selectedtextcolor="#FF324F6D" disabledtextcolor="#FFbac0c5" selectedimage="file='default\radio_sel.png' dest='6,6,19,19'" normalimage="file='default\radio_non.png' dest='6,6,19,19'" align="left" group="ipconfig_ip_type" selected="true" />
				<CheckBox name="ipconfig_ip_manual" height="25" textpadding="25,5,0,0" textcolor="#FF324F6D" hottextcolor="#FF0000FF" selectedtextcolor="#FF324F6D" disabledtextcolor="#FFbac0c5" selectedimage="file='default\radio_sel.png' dest='6,6,19,19'" normalimage="file='default\radio_non.png' dest='6,6,19,19'" align="left" group="ipconfig_ip_type" />
				<VerticalLayout name="ipconfig_ip_manual_setting" >
					<Control height="3" />
					<HorizontalLayout height="24" bordersize="1" bordercolor="FFE0E0E0" >
						<Control width="15" />
						<Label name="ipconfig_s_ip_text" width="100" />
						<Edit name="ipconfig_s_ip_edit" bkcolor="#FFE0E0E0" nativebkcolor="#FFE0E0E0"/>
						<Control width="2" />
					</HorizontalLayout>
					<Control height="3" />
					<HorizontalLayout height="24" bordersize="1" bordercolor="FFE0E0E0" >
						<Control width="15" />
						<Label name="ipconfig_s_mask_text" width="100" />
						<Edit name="ipconfig_s_mask_edit" bkcolor="#FFE0E0E0" nativebkcolor="#FFE0E0E0"/>
						<Control width="2" />
					</HorizontalLayout>
					<Control height="3" />
					<HorizontalLayout height="24" bordersize="1" bordercolor="FFE0E0E0" >
						<Control width="15" />
						<Label name="ipconfig_s_gateway_text" width="100" />
						<Edit name="ipconfig_s_gateway_edit" bkcolor="#FFE0E0E0" nativebkcolor="#FFE0E0E0"/>
						<Control width="2" />
					</HorizontalLayout>
				</VerticalLayout>
			</VerticalLayout>
			<Control height="15" />
			<VerticalLayout height="120" bordersize="1" bordercolor="FFE0E0E0" >
				<CheckBox name="ipconfig_dns_auto" height="25" textpadding="25,5,0,0" textcolor="#FF324F6D" hottextcolor="#FF0000FF" selectedtextcolor="#FF324F6D" disabledtextcolor="#FFbac0c5" selectedimage="file='default\radio_sel.png' dest='6,6,19,19'" normalimage="file='default\radio_non.png' dest='6,6,19,19'" align="left" group="ipconfig_dns_type" selected="true" />
				<CheckBox name="ipconfig_dns_manual" height="25" textpadding="25,5,0,0" textcolor="#FF324F6D" hottextcolor="#FF0000FF" selectedtextcolor="#FF324F6D" disabledtextcolor="#FFbac0c5" selectedimage="file='default\radio_sel.png' dest='6,6,19,19'" normalimage="file='default\radio_non.png' dest='6,6,19,19'" align="left" group="ipconfig_dns_type" />
				<VerticalLayout name="ipconfig_dns_manual_setting" >
					<Control height="3" />
					<HorizontalLayout height="24" bordersize="1" bordercolor="FFE0E0E0" >
						<Control width="15" />
						<Label name="ipconfig_s_dns_text" width="100" />
						<Edit name="ipconfig_s_dns_edit" bkcolor="#FFE0E0E0" nativebkcolor="#FFE0E0E0" text="" />
						<Control width="2" />
					</HorizontalLayout>
					<Control height="3" />
					<HorizontalLayout height="24" bordersize="1" bordercolor="FFE0E0E0" >
						<Control width="15" />
						<Label name="ipconfig_s_dns2_text" width="100" />
						<Edit name="ipconfig_s_dns2_edit" bkcolor="#FFE0E0E0" nativebkcolor="#FFE0E0E0" text="" />
						<Control width="2" />
					</HorizontalLayout>
					<Control height="3" />
				</VerticalLayout>
			</VerticalLayout>
			<Control />
			<HorizontalLayout height="24" bordersize="1" bordercolor="#FFE0E0E0" >
				<Control />
				<Button name="ipconfig_new" width="70" normalimage="file='default\btn_normal.png'" hotimage="file='default\btn_hot.png'" pushedimage="file='default\btn_push.png'" bordersize="1" bordercolor="#FFE8E8E8" borderround="2,2,2,2" />
				<Control />
				<Button name="ipconfig_save" width="90" normalimage="file='default\btn_normal.png'" hotimage="file='default\btn_hot.png'" pushedimage="file='default\btn_push.png'" bordersize="1" bordercolor="#FFE8E8E8" borderround="2,2,2,2" />
				<Control />
				<Button name="ipconfig_delete" width="70" normalimage="file='default\btn_normal.png'" hotimage="file='default\btn_hot.png'" pushedimage="file='default\btn_push.png'" bordersize="1" bordercolor="#FFE8E8E8" borderround="2,2,2,2" />
				<Control />
				<Button name="ipconfig_apply" width="70" normalimage="file='default\btn_normal.png'" hotimage="file='default\btn_hot.png'" pushedimage="file='default\btn_push.png'" bordersize="1" bordercolor="#FFE8E8E8" borderround="2,2,2,2" />
				<Control />
			</HorizontalLayout>
		</VerticalLayout>
		<Control width="2" />
		<VerticalLayout name="ipconfig_info" minwidth="320" bordersize="1" bordercolor="FFE0E0E0" vscrollbar="true">
			<HorizontalLayout height="24" bordersize="1" bordercolor="FFE0E0E0">
				<Control width="15"/>
				<Label name="ipconfig_network_adapters_text" width="100" />
				<Combo name="ipconfig_network_adapters" tooltip="" normalimage="file='default\combo_normal.png' corner='2,2,24,2'" hotimage="file='default\combo_hover.png' corner='2,2,24,2'" pushedimage="file='default\combo_hover.png' corner='2,2,24,2'" itemtextpadding="3,3,3,3" >
				</Combo>
				<Control width="10" />
			</HorizontalLayout>
			<Control height="5" />
			<VerticalLayout height="260" bordersize="1" bordercolor="#FFE0E0E0" >
				<!--网卡描述-->
				<HorizontalLayout height="24" bordersize="1" bordercolor="FFE0E0E0" >
					<Control width="15" />
					<Label name="ipconfig_i_desc_text" width="100" />
					<Edit name="ipconfig_i_desc_edit" bkcolor="#FFE0E0E0" nativebkcolor="#FFE0E0E0" readonly="true" hscrollbar="true"/>
					<Control width="2" />
				</HorizontalLayout>
				<Control height="3" />
				<!--适配器类型-->
				<HorizontalLayout height="24" bordersize="1" bordercolor="FFE0E0E0" >
					<Control width="15" />
					<Label name="ipconfig_i_type_text" width="100" />
					<Edit name="ipconfig_i_type_edit" bkcolor="#FFE0E0E0" nativebkcolor="#FFE0E0E0" readonly="true" />
					<Control width="2" />
				</HorizontalLayout>
				<Control height="3" />
				<!--DHCP-->
				<HorizontalLayout height="24" bordersize="1" bordercolor="FFE0E0E0" >
					<Control width="15" />
					<Label name="ipconfig_i_dhcp_text" width="100" />
					<Edit name="ipconfig_i_dhcp_edit" bkcolor="#FFE0E0E0" nativebkcolor="#FFE0E0E0" readonly="true" />
					<Control width="2" />
				</HorizontalLayout>
				<Control height="3" />
				<!--IP(可能有多个)-->
				<HorizontalLayout height="24" minheight="24" maxheight="48" bordersize="1" bordercolor="FFE0E0E0" >
					<Control width="15" />
					<Label name="ipconfig_i_ip_text" width="100" />
					<Edit name="ipconfig_i_ip_edit" bkcolor="#FFE0E0E0" nativebkcolor="#FFE0E0E0" readonly="true" multiline="true"/>
					<Control width="2" />
				</HorizontalLayout>
				<Control height="3" />
				<!--子网掩码-->
				<HorizontalLayout height="24" bordersize="1" bordercolor="FFE0E0E0" >
					<Control width="15" />
					<Label name="ipconfig_i_mask_text" width="100" />
					<Edit name="ipconfig_i_mask_edit" bkcolor="#FFE0E0E0" nativebkcolor="#FFE0E0E0" readonly="true" />
					<Control width="2" />
				</HorizontalLayout>
				<Control height="3" />
				<!--网关-->
				<HorizontalLayout height="24" bordersize="1" bordercolor="FFE0E0E0" >
					<Control width="15" />
					<Label name="ipconfig_i_netgate_text" width="100" />
					<Edit name="ipconfig_i_netgate_edit" bkcolor="#FFE0E0E0" nativebkcolor="#FFE0E0E0" readonly="true" />
					<Control width="2" />
				</HorizontalLayout>
				<Control height="3" />
				<!--DNS-->
				<HorizontalLayout height="24" bordersize="1" bordercolor="FFE0E0E0" >
					<Control width="15" />
					<Label name="ipconfig_i_dns_text" width="100" />
					<Edit name="ipconfig_i_dns_edit" bkcolor="#FFE0E0E0" nativebkcolor="#FFE0E0E0" readonly="true" />
					<Control width="2" />
				</HorizontalLayout>
				<Control height="3" />
				<!--DNS2-->
				<HorizontalLayout height="24" bordersize="1" bordercolor="FFE0E0E0" >
					<Control width="15" />
					<Label name="ipconfig_i_dns2_text" width="100" />
					<Edit name="ipconfig_i_dns2_edit" bkcolor="#FFE0E0E0" nativebkcolor="#FFE0E0E0" readonly="true" />
					<Control width="2" />
				</HorizontalLayout>
				<Control height="3" />
				<!--MAC-->
				<HorizontalLayout height="24" bordersize="1" bordercolor="FFE0E0E0" >
					<Control width="15" />
					<Label name="ipconfig_i_mac_text" width="100" />
					<Edit name="ipconfig_i_mac_edit" bkcolor="#FFE0E0E0" nativebkcolor="#FFE0E0E0" readonly="true" />
					<Control width="2" />
				</HorizontalLayout>
				<Control height="3" />
			</VerticalLayout>
		</VerticalLayout>
	</HorizontalLayout>
</Window>